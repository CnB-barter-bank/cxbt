    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "3.2.41",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "eth-gas-reporter": "^0.2.27",
    "ethereum-waffle": "^4.0.10",
    "hardhat-deploy-ethers": "^0.4.1",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.5",
    "solidity-coverage": "^0.8.1",
    "tabookey-gasless": "^0.4.5",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3",
    "viem": "^1.15.1",
    "vite": "^3.2.2",
    "vue-tsc": "^1.0.9"
  },
  "scripts": {
    "test": "hardhat test",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && yarn typechain",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && yarn typechain",
    "deploy:sepolia": "npx hardhat deploy  --network sepolia",
    "deploy:bsc": "npx hardhat deploy  --network bsc",
    "deploy:mainnet": "npx hardhat deploy  --network mainnet",
    "deploy:avalanche": "npx hardhat deploy  --network avalanche",
    "deploy:polygon": "npx hardhat deploy  --network polygon",
    "deploy:sepolia:only": "npx hardhat deploy  --network sepolia --tags ",
    "deploy:bsc:only": "npx hardhat deploy  --network bsc --tags ",
    "deploy:mainnet:only": "npx hardhat deploy  --network mainnet --tags ",
    "deploy:avalanche:only": "npx hardhat deploy  --network avalanche --tags ",
    "deploy:polygon:only": "npx hardhat deploy  --network polygon-mainnet --tags ",
    "verify:sepolia:only": "npx hardhat verify --network sepolia",
    "verify:sepolia": "npx hardhat verify --network sepolia",
    "verify:bsc": "npx hardhat verify  --network bsc",
    "verify:mainnet": "npx hardhat verify  --network mainnet",
    "verify:avalanche": "npx hardhat verify  --network avalanche",
    "verify:polygon": "npx hardhat verify  --network polygon-mainnet",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "master": "DEPLOYER=master yarn",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postcompile": "yarn typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "nonce": "npx hardhat nonce --address "
  },
  "dependencies": {
    "@ionic/vue": "^6.3.4",
    "@ionic/vue-router": "^6.3.4",
    "@kolirt/vue-web3-auth": "^2.0.1",
    "@openzeppelin/contracts": "^5.0.1",
    "@openzeppelin/contracts-upgradeable": "^5.0.1",
    "@openzeppelin/gsn-helpers": "^0.2.4",
    "@openzeppelin/upgrades": "^2.8.0",
    "@pancakeswap/pancake-swap-lib": "^0.0.4",
    "@pancakeswap/sdk": "^5.7.3",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.1",
    "ganache-cli": "^6.12.2",
    "ganache-core": "^2.13.2",
    "hardhat": "^2.19.4",
    "hardhat-deploy": "^0.11.45",
    "hardhat-gas-reporter": "^1.0.8",
    "http-server": "^14.1.1",
    "ionicons": "^6.0.3",
    "nodemon": "^3.0.2",
    "pancake-contracts": "https://github.com/pancakeswap/pancake-smart-contracts.git",
    "pancakeswap-peripheral": "https://github.com/pancakeswap/pancake-swap-periphery.git",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  }
